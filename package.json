{
  "name": "laravel-route-to-controller-method-finder",
  "displayName": "Laravel route to controller method Finder",
  "description": "It will help to find the controller method from line from route files",
  "version": "0.0.1",
  "repository": "https://github.com/rudSarkar/laravel-route-to-method",
  "publisher": "rudSarkar",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
		"PHP",
		"Laravel",
		"Blade",
		"View",
		"Namespace",
		"Controller"
	],
  "activationEvents": [
    
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravelcontrollermethodfinder.findLaravelRoutesToController",
        "title": "Find Laravel Controller from routes"
      },
      {
        "command": "laravelcontrollermethodfinder.goToController",
        "title": "Go to controller method"
      }
    ],
    "menus": {
      "editor/context": [
          {
            "when": "resourceFilename =~ /.*\\.php$/ && resourcePath =~ /.*[\\\\/]routes[\\\\/].*/",
            "command": "laravelcontrollermethodfinder.goToController",
            "group": "navigation"
          }
      ],
      "keybindings": [
        {
          "command": "laravelcontrollermethodfinder.findLaravelRoutesToController",
          "key": "ctrl+alt+r",
          "mac": "cmd+alt+r",
          "when": "editorTextFocus"
        },
        {
          "command": "laravelcontrollermethodfinder.goToController",
          "key": "ctrl+alt+g",
          "mac": "cmd+alt+g",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
